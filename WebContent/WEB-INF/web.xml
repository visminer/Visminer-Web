<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>VisMiner Web</display-name>
  <filter>
    <filter-name>Filter</filter-name>
    <filter-class>org.visminer.web.filter.Main</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Filter</filter-name>
    <url-pattern>*.do</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>index</servlet-name>
    <servlet-class>org.visminer.web.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>metric</servlet-name>
    <servlet-class>org.visminer.web.servlet.MetricServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>zoomable</servlet-name>
    <url-pattern>/zoomable.do</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>zoomable</servlet-name>
    <servlet-class>org.visminer.web.servlet.ZoomableCirclePackingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>metric</servlet-name>
    <url-pattern>/metric.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>config</servlet-name>
    <servlet-class>org.visminer.web.servlet.ConfigServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>config</servlet-name>
    <url-pattern>/index.cfg</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>chart</servlet-name>
    <servlet-class>org.visminer.web.servlet.ChartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>chart</servlet-name>
    <url-pattern>/chart.json</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>labelIssuesStatus</servlet-name>
    <servlet-class>org.visminer.web.servlet.IssuesOpenedClosedByLabelController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>labelIssuesStatus</servlet-name>
    <url-pattern>/issuesStatus.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>labelIssueQuantity</servlet-name>
    <servlet-class>org.visminer.web.servlet.LabelIssueQuantityController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>labelIssueQuantity</servlet-name>
    <url-pattern>/issuesQuantity.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>welcome</servlet-name>
    <servlet-class>org.visminer.web.servlet.WelcomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>welcome</servlet-name>
    <url-pattern>/welcome.do</url-pattern>
  </servlet-mapping>
</web-app>